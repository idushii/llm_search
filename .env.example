# Aitunnel
AITUNNEL_API_KEY=sk-aitunnel-***
AITUNNEL_MODEL=gemini-2.0-flash-001

# SearchXNg
SEARCHXNG_BASIC_AUTH_LOGIN=***
SEARCHXNG_BASIC_AUTH_PASSWORD=***


# Лимиты запросов
# SearchXNg: 10 запросов в минуту с равным интервалом
LIMIT_SEARCHXNG_RPM=10

# Jina: 5 запросов в секунду
LIMIT_JINA_RPS=5

# Aitunnel: 2 запроса в секунду
LIMIT_AITUNNEL_RPS=2

# Пути для кэширования
CACHE_DIR=cache
DOCS_DIR=docs
SUMMARIES_DIR=summaries
SEARCH_QUERIES_DIR=search_queries
SEARCH_RESULTS_DIR=search_results
RANKED_RESULTS_DIR=ranked_results
RANKED_SUMMARIES_DIR=ranked_summaries

# Максимальный возраст кэша в днях
MAX_CACHE_AGE_DAYS=7

# Модели для запросов
TOPIC_MODEL=gemini-2.0-flash-001
SEARCH_MODEL=gemini-2.0-flash-001
RANKING_MODEL=gemini-2.0-flash-001
SUMMARY_MODEL=gemini-2.0-flash-001
ANSWER_MODEL=gemini-2.0-flash-001

# Настройки для логирования
LOG_LEVEL=INFO
LOG_FILE=app.log

# Таймауты для HTTP-запросов (в секундах)
HTTP_TIMEOUT=30
SEARCH_TIMEOUT=60

# URL для API
AITUNNEL_API_URL=https://api.aitunnel.ru/v1/chat/completions
SEARCHXNG_API_URL=https://searxng.***
JINA_API_URL=https://r.jina.ai

